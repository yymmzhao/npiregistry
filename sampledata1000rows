    print(f"Columns to extract: {len(columns_to_keep)}")
    
    try:
        # Read only first 1000 rows with selected columns
        print("\nReading first 1000 rows...")
        
        df = pd.read_csv(
            original_file, 
            nrows=1000,
            usecols=columns_to_keep,
            dtype=str  # Keep everything as string for simplicity
        )
        
        print(f"Successfully read {len(df)} rows with {len(df.columns)} columns")
        
        # Save to new file
        df.to_csv(output_file, index=False)
        
        print(f"‚úÖ Sample file created: {output_file}")
        print(f"File size: {os.path.getsize(output_file) / (1024*1024):.2f} MB")
        
        # Show some basic info about the data
        print(f"\nData preview:")
        print(f"Total rows: {len(df)}")
        print(f"Total columns: {len(df.columns)}")
        print(f"\nFirst few NPIs:")
        print(df['NPI'].head().tolist())
        
        print(f"\nSample provider names:")
        provider_names = df['Provider Organization Name (Legal Business Name)'].dropna().head(5)
        for name in provider_names:
            print(f"  {name}")
            
        return True
        
    except Exception as e:
        print(f"‚ùå Error: {e}")
        return False

